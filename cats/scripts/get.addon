function GET(key) {
    var url = document.URL;
    var res = {};
    if (url.split('?')[1] != undefined) {
        var items = url.split('?')[1].split('&');   

        items.forEach(function(val) {
            res[String(val.split('=')[0])] = String(val.split('=')[1]);
        });
    }

    return res[key] ? res[key] : 'Invalid Key';
}